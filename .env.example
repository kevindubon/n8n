# N8N Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_secure_password

# Database Configuration (Supabase)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=your_supabase_host.supabase.co
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=postgres
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=your_supabase_password
DB_POSTGRESDB_SSL_CA=
DB_POSTGRESDB_SSL_CERT=
DB_POSTGRESDB_SSL_KEY=
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false

# N8N Security
N8N_ENCRYPTION_KEY=your_32_character_encryption_key
N8N_USER_MANAGEMENT_JWT_SECRET=your_jwt_secret

# N8N Host Configuration
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-app.vercel.app

# Email Configuration (Optional)
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.gmail.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=your_email@gmail.com
N8N_SMTP_PASS=your_app_password
N8N_SMTP_SENDER=your_email@gmail.com
N8N_SMTP_SSL=false

# Execution Data Cleanup
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336

# Node Environment
NODE_ENV=production