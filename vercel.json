{
  "version": 2,
  "builds": [
    {
      "src": "packages/cli/bin/n8n",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/webhook/(.*)",
      "dest": "packages/cli/bin/n8n"
    },
    {
      "src": "/rest/(.*)",
      "dest": "packages/cli/bin/n8n"
    },
    {
      "src": "/(.*)",
      "dest": "packages/cli/bin/n8n"
    }
  ]
}